<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yuncool.yun_android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:label="首页">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.LoginActivity"
            android:label="登录"/>
        <activity
            android:name=".activity.RegisterActivity"
            android:label="注册"/>
        <activity
            android:name=".activity.ShopDetailActivity"
            android:label="商铺详情"/>
        <activity
            android:name=".activity.MyDocumentActivity"
            android:label="我的文档"/>
        <activity
            android:name=".activity.DocumentDetailActivity"
            android:label="文档详情"/>
        <activity
            android:name=".activity.UserInfoComplementActivity"
            android:label="完善资料"/>
        <activity
            android:name=".activity.RechargeOnlineActivity"
            android:label="在线充值"/>
        <activity
            android:name=".activity.FileUploadActivity"
            android:label="上传文件"/>
        <activity
            android:name=".activity.MircroLibraryActivity"
            android:label="微信资料库"/>
        <activity
            android:name=".activity.RecommendActivity"
            android:label="推荐好友"></activity>
    </application>

</manifest>
