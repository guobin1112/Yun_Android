<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yuncool.yun_android">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.MainActivity"
            android:label="首页">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name=".Activity.LoginActivity"
            android:label="登录" />
        <activity
            android:name=".Activity.RegisterActivity"
            android:label="注册" />
        <activity
            android:name=".Activity.ShopDetailActivity"
            android:label="商铺详情"
            android:windowSoftInputMode="stateHidden">

        </activity>

        <activity
            android:name=".Activity.MyDocumentActivity"
            android:label="我的文档" />
        <activity
            android:name=".Activity.DocumentDetailActivity"
            android:label="文档详情" />
        <activity
            android:name=".Activity.UserInfoComplementActivity"
            android:label="完善资料" />
        <activity
            android:name=".Activity.RechargeOnlineActivity"
            android:label="在线充值" />
        <activity
            android:name=".Activity.FileUploadActivity"
            android:label="上传文件" />
        <activity
            android:name=".Activity.MicroLibraryActivity"
            android:label="微信资料库" />
        <activity
            android:name=".Activity.RecommendActivity"
            android:label="推荐好友" />
        <activity android:name=".Activity.PrintActivity" />
        <activity android:name=".Activity.SocialActivity" />
        <activity android:name=".Activity.SignInActivity" />
        <activity android:name=".Activity.MicroLibListActivity" />
        <activity android:name=".Activity.ShareLinksActivity"></activity>
        <activity android:name=".Activity.VisitShopActivity"></activity>
        <activity android:name=".Activity.EarnYunMoneyActivity"></activity>
    </application>

</manifest>
